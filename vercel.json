{
  "version": 2,
  "builds": [
    { "src": "ReddyAvenue/package.json", "use": "@vercel/static-build" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}




