{
  "builds": [
    {
      "src": "api/index.js",   
      "use": "@vercel/node"
    },
    {
      "src": "ReddyAvenue/package.json",   
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"   
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"  
    },
    {
      "source": "/(.*)",
      "destination": "/ReddyAvenue/dist/index.html"   
    }
  ]
}
